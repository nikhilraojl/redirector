{
  "manifest_version": 2,
  "name": "ReDirector",
  "version": "1.4",
  "description": "Redirect URLs",
  "icons": {
    "48": "icons/redirector_48.png"
  },
  "content_scripts": [
    {
      "matches": [
        "*://*.reddit.com/*",
        "*://*.youtube.com/*",
        "*://*.discord.com/*"
      ],
      "js": ["index.js"],
      "run_at": "document_start"
    }
  ]
}
